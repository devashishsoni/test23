<?xml version="1.0" standalone="no"?>
<!DOCTYPE svg PUBLIC "-//W3C//DTD SVG 1.1//EN" "http://www.w3.org/Graphics/SVG/1.1/DTD/svg11.dtd">
<svg width="300px" height="200px" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
  <!-- Background Rectangle -->
  <rect x="50" y="50" width="200" height="100" fill="red" />
  
  <!-- Warning Text -->
  <text x="150" y="90" font-family="Arial" font-size="20" font-weight="bold" text-anchor="middle" fill="red">
    WARNING!
  </text>
  
  <!-- URL Text -->
  <a xlink:href="https://evil.com" target="_blank">
    <text x="150" y="120" font-family="Arial" font-size="14" text-anchor="middle" fill="white">
      go to https://evil.com
    </text>
  </a>
</svg>
